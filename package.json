{
	"name": "oauth2",
	"version": "6.0.0",
	"description": "The node-red-contrib-oauth2 is a Node-RED node that provides an OAuth2 authentication flow. This node uses the OAuth2 protocol to obtain an access token, which can be used to make authenticated API requests.",
	"author": "Marcos Caputo <caputo.marcos@gmail.com>",
	"license": "MIT License",
	"homepage": "https://github.com/caputomarcos/node-red-contrib-oauth2-api#readme",
	"main": "oauth2.js",
	"repository": {
		"type": "git",
		"url": "git@github.com:caputomarcos/node-red-contrib-oauth2-api.git"
	},
	"bugs": {
		"url": "https://github.com/caputomarcos/node-red-contrib-oauth2-api/issues"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"keywords": [
		"node-red",
		"oauth2",
		"security"
	],
	"node-red": {
		"version": ">=3.0.0",
		"nodes": {
			"oauth2": "src/oauth2.js"
		}
	},
	"dependencies": {
		"@babel/preset-env": "^7.21.5",
		"axios": ">=1.3.3",
		"gitpod-plugin-for-oauth2-red": "file:gitpod-plugin",
		"json-schema": ">=0.4.0",
		"jsonwebtoken": "^9.0.0"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.21.8",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.59.6",
		"babel-eslint": "^10.1.0",
		"carbon-components-svelte": "^0.67.9",
		"carbon-icons-svelte": "^11.1.0",
		"d3": "^7.7.0",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsdoc": "^44.2.4",
		"eslint-plugin-n": "^15.7.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-svelte3": "^4.0.0",
		"i18n": "^0.15.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.0",
		"standard": "^16.0.3",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"svelte-i18n": "^3.6.0",
		"svelte-integration-red": "^1.10.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3"
	},
	"scripts": {
		"build": "sir",
		"update": "./scripts/save.sh",
		"setup": "./scripts/setup.sh",
		"dev": "./scripts/dev.sh",
		"fix": "npx eslint . && npx eslint --fix",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	}
}
